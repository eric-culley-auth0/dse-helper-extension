{
    "manifest_version": 3,
    "name": "Developer Support ESD Extension",
    "description": "Engineering Escalation Extension",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "./public/auth0-logo.png"
    },
    "icons": {
      "16": "./public/auth0-logo.png",
      "32": "./public/auth0-logo.png",
      "48": "./public/auth0-logo.png",
      "128": "./public/auth0-logo.png"
    },
    "permissions": [
      "tabs",
      "identity"
    ],
    "oauth2": {
      "client_id": "30946108485-25fvbu1v7hmgcfd7h698ik1m86ib8tl8.apps.googleusercontent.com",
      "scopes":[
        "https://www.googleapis.com/auth/userinfo.email",
        "https://www.googleapis.com/auth/drive"
      ]
    },
    "key": "-----BEGIN PUBLIC KEY----------END PUBLIC KEY-----",
    "background": {
      "service_worker": "./dist/background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://auth0.lightning.force.com/lightning/r/Case/*"],
        "js": ["./dist/salesforce-script.js"]
      }
    ]
  }